      _analyticsUserId: "ad4842e5-3bee-443b-882d-52d944dd38b9"
  "autocomplete-plus":
    confirmCompletion: "enter"
  "command-palette":
    preserveLastSearch: true
  core:
    allowPendingPaneItems: false
    autoHideMenuBar: true
    closeEmptyWindows: false
